{"components":[{"type":"textarea","id":"text","label":"Text area","placeholder":"Placeholder","isRequired":false,"showError":false,"value":"","min":0,"max":512,"hidden":false,"exclude":false,"invalid":false},{"type":"webComponent","id":"web","source":"code","value":"<style>\n    body {\n        font-family: Euclid Circular A,sans-serif;\n        margin: 0;\n        padding: 10px;\n    }\n    .container {\n        background-color: #C9FFC0;\n        border-radius: 16px;\n        box-shadow: 5px 5px 5px #E5E5E5;\n        box-sizing: border-box;\n        color: #FFFFFF;\n        padding: 18px;\n        width: 100%;\n    }\n    .sa-text {\n        border-color: #E5E5E5;\n        border-radius: 8px;\n        box-sizing: border-box;\n        width: 100%;\n    }\n</style>\n\n<div class=\"container\">\n    <h1>Web Component</h1>\n    <textarea class=\"sa-text\" id=\"sa-text\" rows=\"6\"></textarea>\n</div>\n<script>\n    const textEl = document.getElementById(\"sa-text\");\n    window.addEventListener('message', async e => {\n        textEl.value = e.data.data;\n    });\n\n    textEl.addEventListener(\"input\", (e) => {\n        console.log(e.target.value)\n        window.parent.postMessage({ action: 'topy', data: e.target.value }, \"*\");\n    });\n</script>","height":300,"exclude":true,"invalid":false}],"code":"from typing import List, Union\nfrom accessor import setValue, postMessageToWebComponent\n\ndef on_text_change(path: List[Union[str, int]], value):\n    postMessageToWebComponent(['web'], value)\n    return\n\ndef on_web_message(path: List[Union[str, int]], value):\n    setValue(['text'], value)\n    return\n","environments":[]}